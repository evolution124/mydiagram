        <mxCell id="WjL_fl-T6VKGcTkqsZ7i-54" parent="WjL_fl-T6VKGcTkqsZ7i-51" style="text;align=left;verticalAlign=top;spacingLeft=4;spacingRight=4;overflow=hidden;rotatable=0;points=[[0,0.5],[1,0.5]];portConstraint=eastwest;whiteSpace=wrap;html=1;fillColor=#f5f5f5;fontColor=#333333;strokeColor=#666666;" value="&lt;div&gt;&amp;nbsp; if (!state.debug_mode) {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; if (halt_request == HR_REGULAR) {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; enter_debug_mode(DCSR_CAUSE_DEBUGINT);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; } else if (halt_request == HR_GROUP) {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; enter_debug_mode(DCSR_CAUSE_GROUP);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; } // !!!The halt bit in DCSR is deprecated.&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; else if (state.dcsr-&amp;gt;halt) {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; enter_debug_mode(DCSR_CAUSE_HALT);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; }&lt;/div&gt;&lt;div&gt;&amp;nbsp; }&lt;/div&gt;" vertex="1">

        <mxCell id="WjL_fl-T6VKGcTkqsZ7i-44" parent="WjL_fl-T6VKGcTkqsZ7i-36" style="text;align=left;verticalAlign=top;spacingLeft=4;spacingRight=4;overflow=hidden;rotatable=0;points=[[0,0.5],[1,0.5]];portConstraint=eastwest;whiteSpace=wrap;html=1;fillColor=#f5f5f5;fontColor=#333333;strokeColor=#666666;" value="&lt;div&gt;&amp;nbsp; &amp;nbsp; &lt;span style=&quot;background-color: rgb(185, 224, 165);&quot;&gt;rvfi.dbg_mode = this-&amp;gt;get_state()-&amp;gt;debug_mode;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &lt;span style=&quot;background-color: rgb(185, 224, 165);&quot;&gt;rvfi.mode = this-&amp;gt;get_state()-&amp;gt;last_inst_priv;&amp;nbsp;&lt;/span&gt; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;//设置当前 priv 模式&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &lt;span style=&quot;background-color: light-dark(#ffffff, var(--ge-dark-color, #121212));&quot;&gt;rvfi.insn =&amp;nbsp;&lt;/span&gt;&lt;font style=&quot;&quot;&gt;&lt;font style=&quot;&quot;&gt;&lt;span&gt;&lt;span style=&quot;background-color: light-dark(#ffffff, var(--ge-dark-color, #121212));&quot;&gt;&lt;span style=&quot;background-color: light-dark(#ffffff, var(--ge-dark-color, #121212));&quot;&gt;(uint32_t)(this-&amp;gt;get_state()-&amp;gt;last_inst_fetched.bits() &amp;amp; 0xffffffffULL)&lt;/span&gt;&lt;span style=&quot;background-color: light-dark(#ffffff, var(--ge-dark-color, #121212));&quot;&gt;;&lt;/span&gt;&lt;/span&gt;&amp;nbsp;&lt;/span&gt;&amp;nbsp; //&lt;/font&gt;&amp;nbsp;&lt;/font&gt;&lt;span style=&quot;background-color: transparent; font-size: 12px;&quot;&gt;processor_t::step(n); 更新&lt;/span&gt;&lt;br&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;div style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; // TODO FIXME Handle multiple/zero writes in a single insn.&lt;/div&gt;&lt;div style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; auto &amp;amp;reg_commits = this-&amp;gt;get_state()-&amp;gt;log_reg_write;&lt;/div&gt;&lt;div style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; int xlen = this-&amp;gt;get_state()-&amp;gt;last_inst_xlen;&lt;/div&gt;&lt;div style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; int flen = this-&amp;gt;get_state()-&amp;gt;last_inst_flen;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;br&gt;&lt;div style=&quot;&quot;&gt;&amp;nbsp; &amp;nbsp; &lt;span style=&quot;background-color: rgb(185, 224, 165);&quot;&gt;rvfi.rs1_addr = this-&amp;gt;get_state()-&amp;gt;last_inst_fetched.rs1();&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span style=&quot;background-color: rgb(185, 224, 165);&quot;&gt;rvfi.rs1_rdata = this-&amp;gt;get_XPR(reference-&amp;gt;rs1_addr);&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span style=&quot;background-color: rgb(185, 224, 165);&quot;&gt;rvfi.rs2_addr = this-&amp;gt;get_state()-&amp;gt;last_inst_fetched.rs2();&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span style=&quot;background-color: rgb(185, 224, 165);&quot;&gt;rvfi.rs2_rdata = this-&amp;gt;get_XPR(reference-&amp;gt;rs2_addr);&lt;/span&gt;&lt;br&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; bool got_commit = false;&lt;br&gt;&amp;nbsp; &amp;nbsp;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&amp;nbsp; &amp;nbsp; // 如果出现了interrupt&lt;br&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; if (&lt;/span&gt;&lt;span style=&quot;background-color: rgb(185, 224, 165);&quot;&gt;rvfi.intr&lt;/span&gt;) {&lt;/div&gt;&lt;div style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; for (auto &amp;amp;reg : last_log_reg_write) {&lt;/div&gt;&lt;div style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; reg_t addr = reg.first &amp;gt;&amp;gt; 4;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span style=&quot;background-color: rgb(185, 224, 165);&quot;&gt;rvfi.csr_valid[INDEX_CSR(addr)] = 1;&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span style=&quot;background-color: rgb(185, 224, 165);&quot;&gt;rvfi.csr_addr [INDEX_CSR(addr)] = addr;&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span style=&quot;background-color: rgb(185, 224, 165);&quot;&gt;rvfi.csr_wdata[INDEX_CSR(addr)] = reg.second.v[0];&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;&quot;&gt;&lt;span style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span style=&quot;background-color: rgb(185, 224, 165);&quot;&gt;rvfi.csr_wmask[INDEX_CSR(addr)] = -1;&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/div&gt;&lt;div style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; }&lt;/div&gt;&lt;div style=&quot;background-color: transparent;&quot;&gt;&amp;nbsp; &amp;nbsp; last_log_reg_write.clear();&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;" vertex="1">

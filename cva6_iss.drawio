;&amp;nbsp; if (pmp_homogeneous(paddr &amp;amp; ~reg_t(PGSIZE - 1), PGSIZE)) {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; if (type == FETCH) tlb_insn_tag[idx] = expected_tag;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; else if (type == STORE) tlb_store_tag[idx] = expected_tag;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; else tlb_load_tag[idx] = expected_tag;&lt;/div&gt;&lt;div&gt;&amp;nbsp; }&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp;&lt;span style=&quot;background-color: light-dark(rgb(185, 224, 165), rgb(185, 224, 165));&quot;&gt; tlb_data[idx] = entry;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; return entry;&lt;/div&gt;&lt;div&gt;}&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;" vertex="1">

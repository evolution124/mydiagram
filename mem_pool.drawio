        <mxCell id="Rt-lXHL0REA0ntXYS-w8-76" value="/* Check Overflow */&lt;div&gt;if ((memory + Size) &amp;lt; memory) return gcvSTATUS_INVALID_ARGUMENT;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;background-color: rgb(185, 224, 165);&quot;&gt;memory = &lt;b style=&quot;&quot;&gt;untagged_addr&lt;/b&gt;(Memory);&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;" style="text;strokeColor=default;fillColor=none;align=left;verticalAlign=top;spacingLeft=4;spacingRight=4;overflow=hidden;rotatable=1;points=[[0,0.5],[1,0.5]];portConstraint=eastwest;whiteSpace=wrap;html=1;dashed=1;dashPattern=8 8;movable=1;resizable=1;deletable=1;editable=1;locked=0;connectable=1;" parent="Rt-lXHL0REA0ntXYS-w8-69" vertex="1">

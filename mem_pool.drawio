        </mxCell>
        <mxCell id="EAXsDxyp4Mi5biQF-tPH-19" value="&lt;div&gt;&lt;span style=&quot;background-color: rgb(153, 255, 255);&quot;&gt;/* 找到pudStart 与 pudEnd 并计算pudEntries */&lt;/span&gt;&lt;/div&gt;&lt;div&gt;else {&lt;br&gt;&amp;nbsp; &amp;nbsp; dynamicArea = Mmu-&amp;gt;dynamicAreaList;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; if (startAddr &amp;amp; 0x3FFF_FFFF) ONERROR(gcvSTATUS_INVALID_ARGUMENT);&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; while(dynamicArea) {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if (startAddr &amp;gt;= dynamicArea-&amp;gt;baseAddress &amp;amp;&amp;amp;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; startAddr &amp;lt; dynamicArea-&amp;gt;baseAddress + dynamicArea-&amp;gt;vaSize) {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ONERROR(gcvSTATUS_INVALID_REQUEST); ????&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; } else {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; dynamicArea = dynamicArea-&amp;gt;next;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; }&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &lt;span style=&quot;background-color: rgb(213, 232, 212);&quot;&gt;pudStart = (startAddr &amp;amp; gcdMMU_PGD_MASK) &amp;gt;&amp;gt; gcdMMU_PGD_SHIFT) *&amp;nbsp;&lt;/span&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;span style=&quot;background-color: rgb(213, 232, 212);&quot;&gt;gcdMMU_PUD_ENTRY_NUM + (start &amp;amp; gcdMMU_PUD_MASK) &amp;gt;&amp;gt; gcdMMU_PUD_SHIFT);&lt;/span&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; &lt;span style=&quot;background-color: rgb(213, 232, 212);&quot;&gt;pudEnd&amp;nbsp; = pudStart;&lt;/span&gt;&lt;br&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &lt;span style=&quot;background-color: rgb(255, 242, 204);&quot;&gt;/* If need size more than 4G, we need to shorten pudEnd to avoid overlap */&lt;/span&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; for (i = 1; i &amp;lt; pudEntries; i++) {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if (Mmu-&amp;gt;pudLogical[pudStart + i])&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; break;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; pudEnd++;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; }&lt;/div&gt;&lt;div&gt;&lt;span&gt;&amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span style=&quot;background-color: rgb(213, 232, 212);&quot;&gt;pudEntries = pudEnd - pudStart + 1;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;}&lt;/div&gt;" style="text;strokeColor=default;fillColor=none;align=left;verticalAlign=top;spacingLeft=4;spacingRight=4;overflow=hidden;rotatable=0;points=[[0,0.5],[1,0.5]];portConstraint=eastwest;whiteSpace=wrap;html=1;dashed=1;" parent="EAXsDxyp4Mi5biQF-tPH-14" vertex="1">
